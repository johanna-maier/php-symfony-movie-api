{% if id %}
  <h1>Details for movie: {{movieDetails.title}} (ID: {{movieDetails.id}})</h1>
  <p>{{movieDetails.description}}</p>
  <embed width="420" height="315" src="https://www.youtube.com/embed/{{movieDetails.videoKey}}">
  <br>
   <div id="app"></div>

    <script type="text/javascript">
      // Select the div element with 'app' id
      const app = document.getElementById('app');

      // Create a new H1 element
      const header = document.createElement('h1');

      // Create a new text node for the H1 element
      const headerContent = document.createTextNode(
        'Develop. Preview. Ship. ðŸš€',
      );

      // Append the text to the H1 element
      header.appendChild(headerContent);

      // Place the H1 element inside the div
      app.appendChild(header);
    </script>
{% else %}
  <p>Woops, no id is selected title set.</p>
{% endif %}
